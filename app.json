{
  "expo": {
    "name": "App",
    "slug": "App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logopass.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/a.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "android": {
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "marginTop": "auto",
        "marginBottom": "auto",
        "alignItems": "center",
        "justifyContent": "center"
      },
      "ios": {
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "marginTop": "auto",
        "marginBottom": "auto",
        "alignItems": "center",
        "justifyContent": "center"
      }
    },
    "assets": ["./assets/fonts/FranklinGothic.ttf"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logopass.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logopass.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
